<component-set>
	<components>
		<component>
			<role>
				org.apache.maven.artifact.handler.ArtifactHandler
			</role>
			<role-hint>wowebserverresources-artifact</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<classifier>wowebserverresources</classifier>
				<extension>-wowebserverresources.tar.gz</extension>
				<type>wowebserverresources-artifact</type>
				<packaging>wowebserverresources</packaging>
				<language>java</language>
				<addedToClasspath>false</addedToClasspath>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.artifact.handler.ArtifactHandler
			</role>
			<role-hint>woapplication-artifact</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<classifier>woapplication</classifier>
				<extension>-woapplication.tar.gz</extension>
				<type>woapplication-artifact</type>
				<packaging>woapplication</packaging>
				<language>java</language>
				<addedToClasspath>false</addedToClasspath>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.lifecycle.mapping.LifecycleMapping
			</role>
			<role-hint>woapplication</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<validate>
						org.objectstyle.woproject.maven2:maven-wolifecycle-plugin:define-woapplication-resources
					</validate>
					<process-resources>
						org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-test-resources>
						org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>
						org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<test>
						org.apache.maven.plugins:maven-surefire-plugin:test
					</test>
					<generate-test-resources>
						org.objectstyle.woproject.maven2:maven-wolifecycle-plugin:woapplication
					</generate-test-resources>
					<package>
						org.objectstyle.woproject.maven2:maven-wolifecycle-plugin:package-woapplication
					</package>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
					<deploy>
						org.apache.maven.plugins:maven-deploy-plugin:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.artifact.handler.ArtifactHandler
			</role>
			<role-hint>woframework-artifact</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<classifier>woframework</classifier>
				<type>woframework-artifact</type>
				<extension>jar</extension>
				<packaging>woframework</packaging>
				<language>java</language>
				<addedToClasspath>true</addedToClasspath>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.lifecycle.mapping.LifecycleMapping
			</role>
			<role-hint>woframework</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<validate>
						org.objectstyle.woproject.maven2:maven-wolifecycle-plugin:define-woframework-resources
					</validate>
					<process-resources>
						org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-test-resources>
						org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>
						org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<test>
						org.apache.maven.plugins:maven-surefire-plugin:test
					</test>
					<generate-test-resources>
						org.objectstyle.woproject.maven2:maven-wolifecycle-plugin:woframework
					</generate-test-resources>
					<package>
						org.objectstyle.woproject.maven2:maven-wolifecycle-plugin:package-woframework
					</package>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
					<deploy>
						org.apache.maven.plugins:maven-deploy-plugin:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
	</components>
</component-set>
